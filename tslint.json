{
  "extends": ["tslint-config-airbnb", "tslint-config-prettier"],
  "rulesDirectory": ["tslint-plugin-prettier"],
  "rules": {
    "prettier": [true],
    "variable-name": [
      true,
      "ban-keywords",
      "check-format",
      "allow-pascal-case"
    ],
    "import-name": false,
    "no-increment-decrement": false
  }
}
